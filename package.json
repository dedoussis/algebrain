{
  "name": "algebrain",
  "version": "0.0.9",
  "description": "Combuter Algebra System focusing on symbolic transformations",
  "keywords": [
    "algebra",
    "CAS",
    "symbolic",
    "transformation",
    "rewriting",
    "rules",
    "math",
    "mathematics",
    "compiler"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "dedoussis",
  "repository": {
    "type": "git",
    "url": "https://github.com/dedoussis/algebrain"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' 'test/**/*.ts' --fix --quiet",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && typedoc --out docs --target es6 --theme minimal --mode file src",
    "test": "jest --coverage",
    "test:watch": "npm run test --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "antlr4": "antlr4ts -visitor src/parser/Algebrain.g4"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^1.0.1",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^13.1.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/core-js": "^2.5.2",
    "antlr4ts": "^0.5.0-alpha.3",
    "immutable": "^4.0.0-rc.12"
  }
}
